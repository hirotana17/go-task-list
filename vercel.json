{
  "version": 2,
  "builds": [
    {
      "src": "main.go",
      "use": "@vercel/go"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/main.go"
    },
    {
      "src": "/(.*)",
      "dest": "/client/vite-project/dist/$1"
    }
  ],
  "env": {
    "ENV": "production"
  }
} 